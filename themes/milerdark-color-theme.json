{
	"name": "milerdark",
	"type": "dark",
	"colors": {
		"welcomePage.background": "#1a1a1a",
		"titleBar.activeBackground": "#1a1a1a",
		"titleBar.activeForeground": "#fff",
		"titleBar.inactiveBackground": "#222",
		"statusBar.background": "#1a1a1a",
		"statusBarItem.hoverBackground": "#111",
		"editor.background": "#222222",
		"editorCursor.foreground": "#43e3ff",
		"editorGutter.background": "#222",
		"activityBar.background": "#1a1a1a",
		"activityBarBadge.foreground": "#FBFF00",
		"activityBarBadge.background": "#b948ff",
		"sideBarTitle.foreground": "#43e3ff",
		"sideBarSectionHeader.background": "#333",
		"sideBarSectionHeader.foreground": "#fff",
		"sideBar.background": "#222", // asdfasdf
		"sideBar.foreground": "#fff", // asdfasdf
		"contrastBorder": "#1a1a1a",
		"activityBar.foreground": "#43e3ff",
		"activityBar.inactiveForeground": "#666",
		"activityBar.border": "#1a1a1a",
		"activityBar.activeBorder": "#43e3ff",
		"activityBar.activeBackground": "#053139",
		"editorGroupHeader.tabsBackground": "#222",
		"editorGroupHeader.tabsBorder": "#111",
		"tab.activeBackground": "#222",
		"tab.activeForeground": "#fff",
		"tab.activeBorderTop": "#43e3ff",
		"tab.inactiveBackground": "#2b2b2b",
		"tab.hoverBackground": "#222",
		"tab.hoverForeground": "#fff",
		"panel.background": "#222",
		"panel.border": "#111",
		"panelTitle.activeBorder": "#43e3ff",
		"panelTitle.activeForeground": "#43e3ff",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6ea15d"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
				"meta.function.parameters.php"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#78a5ff"
			}
		},
		{
			"name": "Property Value",
			"scope": [
				"support.constant.property-value",
			],
			"settings": {
				"foreground": "#78a5ff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control.from",
				"keyword.control.flow",
				"meta.brace.round",
				"punctuation.definition.binding-pattern.object"
			],
			"settings": {
				"foreground": "#FC92E3"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#E1F4F3"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter",
				"entity.name.type.class.php"
			],
			"settings": {
				"foreground": "#e96f6f"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end",
				"punctuation.definition.keyword",
				"keyword.control.at-rule.media",
				"keyword.control.at-rule.keyframes",
				// "punctuation.definition.binding-pattern.array",
			],
			"settings": {
				"foreground": "#43e3ff"
			}
		},
		{
			"name": "Import, Export",
			"scope": [
				"keyword.control.import",
				"keyword.control.export"
			],
			"settings": {
				"foreground": "#43e3ff"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other",
				"punctuation.definition.variable.php",
			],
			"settings": {
				"foreground": "#43e3ff"
			}
		},
		{
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#909090"
			}
		},
	  	{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link",
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"variable.other.object",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"scope": [
				"keyword.control.conditional",
				"entity.name.type.class",
				"variable.other.php",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"variable.other.global.php",
				"variable.language.this.php",
				"variable.other.global.safer.php",
			],
			"settings": {
				"foreground": "#43e3ff"
			}
		},
		{
			"scope": [
				"variable.other.readwrite.alias",
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
				"variable.other.property",
				"string.template"
			],
			"settings": {
				"foreground": "#ffda90"
			}
		},
